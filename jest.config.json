{
	"roots": ["<rootDir>/src/test/"],
	"testMatch": [
		"<rootDir>/src/test/**/*.test.{js,jsx,ts,tsx}",
		"<rootDir>/src/**/test/**/*.test.{js,jsx,ts,tsx}"
	],
	"moduleNameMapper": {
		"\\.(css|less|svg)$": "identity-obj-proxy"
	},
	"setupFilesAfterEnv": ["<rootDir>/src/test/setup.ts"],
	"transform": {
		"^.+\\.tsx?$": "ts-jest"
	}
}
